    "moment": "2.26.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "3.3.3",
    "vue": "2.6.11",
    "vue-i18n": "8.17.4",
    "vue-router": "3.1.6",
    "vuex": "3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
